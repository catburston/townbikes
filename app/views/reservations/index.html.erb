<h1>Reservation History</h1>
<ul>
  <% @reservations.each do |reservation| %>
    <li>
      <%= link_to "see more details", user_reservation_path(current_user.id, reservation.id) %><br />
      Reservation ID: <%= reservation.id %><br />
      Bicycle ID: <%= reservation.bicycle_id %><br />
      From Date: <%= reservation.from_date %><br />
      To Date: <%= reservation.to_date %><br />
      Bicycle Owner: <%= link_to "#{@owner.first_name} #{@owner.last_name}", user_path(@owner.id) %><br />
      Location: <%= @owner.location %>
    </li>
  <% end %>
</ul>


